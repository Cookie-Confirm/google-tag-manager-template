___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const setDefaultConsentState = require('setDefaultConsentState');
const injectScript = require('injectScript');
const getCookieValues = require('getCookieValues');
const JSON = require('JSON');
const updateConsentState = require('updateConsentState');
const gtagSet = require('gtagSet');
const setInWindow = require('setInWindow');
const consentManager = data.javascript_url;

// Variabele for the consent script to prevent double init states
setInWindow('cookieConfirmGtmInit', true);

gtagSet({
    'developer_id.dOWYzYj': true,
    'url_passthrough': !!data.url_passthrough,
    'ads_data_redaction': !!data.ads_data_redaction,
});


var defaultConsent = {
    ad_storage: data.ad_storage, // Marketing
    ad_user_data: data.ad_user_data, // Marketing
    ad_personalization: data.ad_personalization, // Marketing
    analytics_storage: data.analytics_storage, // Analytics
    functionality_storage: data.functionality_storage, // Functional
    personalization_storage: data.personalization_storage, // Functional
    security_storage: data.security_storage, // Functional
    wait_for_update: data.wait_for_update
};

log(defaultConsent);

setDefaultConsentState(defaultConsent);

var consentString = getCookieValues("CC_p_accepted_cookies")[0] || null,
    consent = JSON.parse(consentString ? consentString : '{}');

if (consent.length) {
    var hasAnalytics = false,
        hasMarketing = false;

    for (var key in consent) {
        hasAnalytics = hasAnalytics || consent[key] === 'analytics';
        hasMarketing = hasMarketing || consent[key] === 'marketing';
    }

    updateConsentState({
        ad_storage: hasMarketing ? 'granted' : 'denied', // Marketing
        ad_user_data: hasMarketing ? 'granted' : 'denied', // Marketing
        ad_personalization: hasMarketing ? 'granted' : 'denied', // Marketing
        analytics_storage: hasAnalytics ? 'granted' : 'denied', // Analytics
        functionality_storage: 'granted', // Functional
        personalization_storage: 'granted', // Functional
        security_storage: 'granted', // Functional
    });
}


// Call data.gtmOnSuccess when completed
injectScript(consentManager, data.gtmOnSuccess);


